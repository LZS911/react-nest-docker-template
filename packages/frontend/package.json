{
  "name": "@haibara/todo-frontend",
  "private": true,
  "version": "0.0.0",
  "author": "ly",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LZS911/react-project-template"
  },
  "scripts": {
    "start": "vite --port=7879 --host=0.0.0.0",
    "build": "npm run checker && npm run test:n && rm -rf dist && tsc && vite build",
    "build:docker": "docker build -t react-docker -f Dockerfile.local .",
    "run:docker": "docker run -p 7880:7879 react-docker",
    "preview": "vite preview",
    "stylelint": " stylelint \"./src\"",
    "cspell": "cspell \"src/**/{*.ts,*.tsx,*.js,*.jsx}\"",
    "eslint": "eslint -c ./.eslintrc.json \"src/**/{*.ts,*.tsx,*.js,*.jsx}\"",
    "checker": "npm-run-all -p eslint cspell stylelint ts-check prettier:c",
    "ts-check": "tsc --noEmit",
    "test": "jest --watchAll",
    "test:c": "jest --coverage",
    "test:n": "jest",
    "prettier:c": "prettier -c src/",
    "prettier:w": "prettier -w src/",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@ant-design/plots": "^1.2.1",
    "@material/react-linear-progress": "^0.15.0",
    "@reduxjs/toolkit": "^1.8.3",
    "ahooks": "^3.5.2",
    "axios": "^0.27.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-grid-layout": "^1.3.4",
    "react-i18next": "^11.17.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.2.0",
    "tailwindcss": "^3.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom-worker": "^0.2.1",
    "postcss": "^8.4.14",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "redux-devtools-extension": "^2.13.9",
    "rollup-plugin-visualizer": "^5.6.0",
    "use-resize-observer": "^9.0.2",
    "vite": "^2.9.9",
    "vite-plugin-conditional-compile": "^1.2.0",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-import": "^0.4.0",
    "vite-plugin-watch-i18": "^1.2.8"
  }
}
